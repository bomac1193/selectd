# SELECTR Environment Variables
# Copy to .env.local and fill in values

# ======================
# DATABASE (Supabase)
# ======================
# Get from: Supabase Dashboard > Project Settings > Database
DATABASE_URL="postgresql://postgres.[project-ref]:[password]@aws-0-[region].pooler.supabase.com:6543/postgres?pgbouncer=true"
DIRECT_URL="postgresql://postgres.[project-ref]:[password]@aws-0-[region].pooler.supabase.com:5432/postgres"

# ======================
# AUTHENTICATION
# ======================
# NextAuth
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="generate-with: openssl rand -base64 32"

# OAuth Providers (at least one required)
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""
DISCORD_CLIENT_ID=""
DISCORD_CLIENT_SECRET=""
DISCORD_PUBLIC_KEY=""
DISCORD_BOT_TOKEN=""
DISCORD_GUILD_ID=""
NEXT_PUBLIC_APP_URL="https://selectr.vercel.app"
NEXT_PUBLIC_DISCORD_INVITE_URL=""
DISCORD_WEBHOOK_URL=""

# ======================
# CANORA INTEGRATION
# ======================
# Set to your deployed CANORA URL
CANORA_API_URL="https://canora.vercel.app"
CANORA_API_KEY=""

# ======================
# ISSUANCE INTEGRATION
# ======================
# Set to your deployed ISSUANCE URL
ISSUANCE_API_URL="https://issuance.vercel.app"
ISSUANCE_API_KEY=""

# ======================
# TELEGRAM MINI APP
# ======================
# Public values for web CTA / links
NEXT_PUBLIC_TELEGRAM_BOT_USERNAME="SelectrBot"
NEXT_PUBLIC_TELEGRAM_APP_SHORT_NAME="selectr"

# Bot server
TELEGRAM_BOT_TOKEN=""
SELECTR_APP_URL="https://selectr.vercel.app"

# ======================
# DEV UTILITIES
# ======================
NGROK_AUTHTOKEN=""
WEB_PORT=3000
